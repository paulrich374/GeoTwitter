.swipe {
  overflow: hidden;
  visibility: hidden;
  position: relative;
}
.swipe-wrap {
  overflow: hidden;
  position: relative;
}
.swipe-wrap > div {
  float:left;
  width:100%;
  position: relative;
  

z-index: 1100;
/*
display: inline-block;
position: absolute;
*/
padding: 0;
outline: 0;
  
  
}
.ui-popup-screen {
background:#000;
background:rgba(0, 0, 0, .7);
top: 0px;
left: 0px;
right: 0px;
bottom: 1px;
position: absolute;
/*
filter: Alpha(Opacity=0);
opacity: 0;
*/
z-index: 1099;
display:none;
}
/* END required styles */

</style>

